  "scripts": {
    "prebuild": "npm run remove-dist",
    "build": "tsc",
    "prestart": "npm run remove-dist",
    "start": "cross-env NODE_ENV=local tsc-watch --project . --onFirstSuccess \"npm run nodemon\"",
    "prenodemon": "npm run copy-resources",
    "nodemon": "nodemon ./dist/src/index.js",
    "nodemon-debug": "npm run nodemon -- --inspect",
    "copy-resources": "cp -Rf ./config ./dist",
    "remove-dist": "rm -rf ./dist",
    "serve": "node ./dist/src/index.js",
    "test": "jest --forceExit --coverage",
    "test:w": "jest --forceExit --watchAll",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "yarn lint -- --fix"
  },
  "dependencies": {
